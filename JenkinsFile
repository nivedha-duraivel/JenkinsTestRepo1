pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
            script
            {
                def Foo = 'foo'
                sh "echo ${Foo}"
            }
                
                echo 'Hello World'
                
            }
        }
         stage('Build') {
            steps {
                echo 'Building job'
            }
        }
         stage('Test') {
            steps {
                echo 'Testing job'
            }
        }
    }
}
